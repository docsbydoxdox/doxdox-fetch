{
  "name": "doxdox-fetch",
  "version": "2.0.0",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "adm-zip": "0.5.16",
    "doxdox-core": "4.0.0-preview.25",
    "node-fetch": "3.3.2"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.7",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.19",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "dotenv": "16.5.0",
    "eslint": "9.27.0",
    "jest": "29.7.0",
    "rimraf": "6.0.1",
    "ts-jest": "29.3.4",
    "ts-jest-resolver": "2.0.1",
    "typescript": "5.8.3"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --setupFiles dotenv/config",
    "lint": "eslint 'src/**/*.ts'"
  },
  "jest": {
    "collectCoverage": true,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "resolver": "ts-jest-resolver",
    "testRegex": ".*.test.ts$"
  },
  "keywords": [
    "doxdox",
    "documentation",
    "fetch"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/docsbydoxdox/doxdox-fetch",
  "repository": {
    "type": "git",
    "url": "git://github.com/docsbydoxdox/doxdox-fetch.git"
  }
}
