{
  "name": "doxdox-fetch",
  "version": "1.0.1",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "adm-zip": "0.5.9",
    "axios": "0.26.0",
    "doxdox-core": "4.0.0-preview.10",
    "doxdox-parser-jsdoc": "4.0.0-preview.10",
    "raspar": "3.0.2"
  },
  "devDependencies": {
    "@types/adm-zip": "0.4.34",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "del": "6.0.0",
    "dotenv": "16.0.0",
    "eslint": "8.10.0",
    "jest": "27.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --setupFiles dotenv/config",
    "lint": "eslint 'src/**/*.ts'"
  },
  "jest": {
    "collectCoverage": true,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleNameMapper": {
      "^.+\\.ts$": "$1"
    },
    "testRegex": ".*.test.ts$"
  },
  "keywords": [
    "doxdox",
    "documentation",
    "fetch"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/docsbydoxdox/doxdox-fetch",
  "repository": {
    "type": "git",
    "url": "git://github.com/docsbydoxdox/doxdox-fetch.git"
  }
}
